{%- for key in ssh_keys['keys'] %}

Host {{ key.hostname }}
  HostName {{ key.hostname }}
  User git
{%- if key.env %}
  IdentityFile /root/.ssh/{{ key.env }}
{%- else -%}
{% set filename = key.path.split('/') %}
  IdentityFile /root/.ssh/{{ filename|last }}
{% endif %}
  IdentitiesOnly yes
{%- endfor %}